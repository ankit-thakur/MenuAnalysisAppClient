{
  "expo": {
    "name": "ANOMO",
    "slug": "anomo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/anomo-logo-2.png",
    "scheme": "anomo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/anomo-logo-text-1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.kittech.anomo",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/anomo-logo-text-1.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/anomo-logo-2.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-barcode-scanner",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "94202f8e-178c-4b0b-a9b5-feacae4948ff"
      }
    },
    "owner": "ankth"
  }
}
